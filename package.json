{
  "name": "MusicMi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run pugw\" \"npm run pugwi\" \"npm run sass\" ",
    "pug-build": "pug pug/main/ --out ./dist/ && pug pug/instruments/ --out ./dist/",
    "pugw": "pug pug/main/ --out ./assets/ --watch",
    "pugwi": "pug pug/instruments/ --out ./assets/instruments/ --watch",
    "sass": "node-sass -w ./sass -o ./assets/css --output-style compressed",
    "sass-build": "node-sass ./sass -o ./dist/css --output-style compressed",
    "test-uncss": "npm run sass-build && npm run uncss",
    "//uncss": "uncss -n --ignore \"/navbar-shrink/,/expanded/,/modal.fade/\" ./dist/*.html --output ./dist/css/styles.css",
    "uncss": "uncss -n --timeout 5000 -u ./uncss.json ./dist/*.html --output ./dist/css/styles.css",
    "css": "npm run sass-build && npm run uncss",
    "build": "npm run pug-build && npm run sass-build && npm run uncss && npm run html-minifier",
    "build-no-uncss": "npm run pug-build && npm run sass-build && npm run html-minifier",
    "html-minifier": "html-minifier-terser --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true ./dist/index.html | sponge ./dist/index.html",
    "wpbuild": "webpack --config webpack.prod.js",
    "wpwatch": "webpack --watch --config webpack.dev.js",

    "bs": "browser-sync start -s ./assets/ --watch *.html,*.js,*.css --startPath ./assets/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.3.0",
    "html-minifier-terser": "^6.0.2",
    "node-sass": "^6.0.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {}
}
