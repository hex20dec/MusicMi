{
  "name": "MusicMi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run pugw\" \"npm run pugwi\" \"npm run sass\" ",
    "pug-build": "pug pug/main/ --out ./dist/ && pug pug/instruments/ --out ./dist/instruments/",
    "pugw": "pug pug/main/ --out ./public/ --watch",
    "pugwi": "pug pug/instruments/ --out ./public/instruments/ --watch",
    "sass": "node-sass -w ./sass -o ./public/css --output-style compressed",
    "sass-build": "node-sass ./sass -o ./dist/css --output-style compressed",
    "test-uncss": "npm run sass-build && npm run uncss",
    "//uncss": "uncss -n --ignore \"/navbar-shrink/,/expanded/,/modal.fade/\" ./dist/*.html --output ./dist/css/styles.css",
    "uncss": "uncss -n --timeout 5000 -u ./uncss.json ./dist/*.html --output ./dist/css/styles.css",
    "css": "npm run sass-build && npm run uncss",
    "build": "npm run pug-build && npm run sass-build && npm run uncss && npm run html-minifier",
    "build-no-uncss": "npm run cp && npm run pug-build && npm run sass-build && npm run html-minifier",
    "html-minifier": "html-minifier-terser --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true --input-dir ./public/ --output-dir ./dist/ --file-ext html",
    "wpbuild": "webpack --config webpack.prod.js",
    "wpwatch": "webpack --watch --config webpack.dev.js",
    "cp": "cp -r -u ./public/* ./dist/",
    "clean": "rm -rf ./dist/*",
    "bs": "browser-sync start -s ./public/ --watch *.html,*.js,*.css",
    "bsbuilt": "browser-sync start -s ./dist/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.3.0",
    "html-minifier-terser": "^6.0.2",
    "node-sass": "^6.0.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {
    "uncss": "^0.17.3"
  }
}
